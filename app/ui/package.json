{
  "name": "server-manager-ui",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.html",
  "scripts": {
    "clean": "rm -rf lib; rm -rf dist",
    "build": "babel src -d lib",
    "lint": "eslint \"src/**/*.js\"",
    "bundle": "npm run build && npx rollup -c; cp -rf src/index.html dist/;cp -rf src/index.css dist/",
    "test": "NODE_ENV=testing mocha tests/ --exit",
    "watch": "npm run bundle; chokidar \"src/**/*.*\" -c \"npm run bundle\"",
    "docker:dev": "docker-compose -f docker-compose.dev.yaml down -v; sudo docker-compose -f docker-compose.dev.yaml up -d",
    "docker:build": "echo 'docker:build'",
    "docker:run": "echo 'docker:run'",
    "release": "standard-version -r",
    "release-rc": "standard-version -p"
  },
  "author": "Thomas Kottke <t.kottke90@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-cli": "^6.26.0",
    "chokidar": "^3.3.1",
    "chokidar-cli": "^2.1.0",
    "eslint": "^6.8.0",
    "rollup": "^1.31.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "socket.io-client": "^2.3.0",
    "standard-version": "^7.1.0"
  },
  "dependencies": {
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2"
  }
}
