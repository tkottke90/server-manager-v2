    "passport-local": "^1.0.0",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "pg-native": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.1.0",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1"
  }
