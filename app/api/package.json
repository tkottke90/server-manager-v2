{
  "name": "server-manager-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "clean": "rm -rf ./dist",
    "build:copy": "cp src/.sequelizerc dist/",
    "build": "tsc; npm run build:copy",
    "sequelize:migrate": "( cd dist/; sequelize db:migrate )",
    "start:dev": "( cd src/; sequelize db:migrate ); nodemon src/",
    "start": "npm run sequelize:migrate; node dist/",
    "release": "standard-version -r",
    "release-rc": "standard-version -p",
    "docker:build": "npm run lint; docker build -t tkottke/server-manager-api:latest ."
  },
  "repository": {},
  "author": "Thomas Kottke <t.kottke90@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/node": "^13.1.8",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.4",
    "@types/validator": "^12.0.1",
    "@types/winston": "^2.4.4",
    "nodemon": "^2.0.2",
    "standard-version": "^8.0.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
